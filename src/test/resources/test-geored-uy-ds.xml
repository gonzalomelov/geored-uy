<?xml version="1.0" encoding="UTF-8"?>
<!-- This is an unmanaged datasource. It should be used for proofs of concept 
   or testing only. It uses H2, an in memory database that ships with JBoss 
   AS. -->
<datasources xmlns="http://www.jboss.org/ironjacamar/schema"
   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation="http://www.jboss.org/ironjacamar/schema http://docs.jboss.org/ironjacamar/schema/datasources_1_0.xsd">
   
   <datasource jndi-name="java:jboss/datasources/test-postgresql-geored-uyDS"
      pool-name="georeduyTestDB" enabled="true"
      use-java-context="true">
      <connection-url>jdbc:postgresql://localhost:5432/georeduyTestDB</connection-url>
      <driver>postgresql</driver>
      <security>
         <user-name>gonza</user-name>
         <password></password>
      </security>
   </datasource>
</datasources>